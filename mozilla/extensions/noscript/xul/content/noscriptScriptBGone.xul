box>  	<hbox>  		<vbox flex="1">  			<label value="&sbgOptionsBlurb;"/>  		</vbox>  		<vbox>  			<button label="&sbgOptionsButton;" oncommand="_noScript_openOptions();"/>  		</vbox>  	</hbox>  </vbox></dialog>